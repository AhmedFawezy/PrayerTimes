<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_height="match_parent"
	android:layout_width="match_parent"
	android:orientation="vertical"
	android:background="#21605B">

	<LinearLayout
		android:layout_height="match_parent"
		android:layout_width="match_parent"
		android:orientation="vertical"
		android:gravity="center"
		android:background="@drawable/bglinmoon">

		<LinearLayout
			android:gravity="center"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:orientation="horizontal"
			android:weightSum="3"
			android:layout_gravity="center">

			<TextView
				android:textSize="30sp"
				android:textColor="#358020"
				android:id="@+id/dhj"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="Text"
				android:layout_weight="1.0"/>

			 
		</LinearLayout>

		<LinearLayout
			android:gravity="center"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:orientation="horizontal"
			android:weightSum="2"
			android:layout_gravity="center">

			<TextView
				android:textSize="30sp"
				android:textColor="#BEE18A"
				android:id="@+id/datem"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="Text"/>

			 

		</LinearLayout>

		<LinearLayout
			android:gravity="center"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:orientation="horizontal"
			android:weightSum="2"
			android:layout_gravity="center">

			<TextView
				android:textSize="30sp"
				android:textColor="#9E9E9E"
				android:id="@+id/datek"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:text="Text"/>

			 

		</LinearLayout>

		 
		<LinearLayout
			android:id="@+id/lin"
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:orientation="vertical">

			<RelativeLayout
				android:padding="16dp"
				android:id="@+id/lin"
				android:layout_width="120dp"
				android:layout_height="120dp"
				android:background="@drawable/bglin1">

				<com.PrayerTimes.StarFieldView
					android:layout_height="300dp"
					android:layout_width="match_parent"
					android:orientation="vertical"/>

				<ImageView
					
					android:id="@+id/imageview1"
					android:layout_height="100dp"
					android:layout_width="100dp"
					android:scaleType="center"
					android:layout_centerInParent="true"
					/>

			</RelativeLayout>

		</LinearLayout>

		<TableLayout
			
			android:layout_height="wrap_content"
			android:layout_width="wrap_content">

			<TableRow
				
				android:layout_height="wrap_content"
				android:layout_width="wrap_content">

				<TextView
					android:paddingLeft="16dp"
					android:paddingRight="16dp"
					android:layout_weight="1.0"
					android:id="@+id/textview2"
					android:textColor="#46AAAB"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="MoonPhases"
					android:layout_below="@id/lin"/>
				<TextView
					android:textSize="5sp"
					android:layout_height="wrap_content"
					android:layout_width="5dp"
					android:text="\t"/>
				<TextView
					android:paddingLeft="16dp"
					android:paddingRight="16dp"
					android:layout_weight="1.0"
					android:id="@+id/textview3"
					android:textColor="#9E9E9E"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="agemoon"/>

			</TableRow>
			<TableRow
				 
				android:layout_height="wrap_content"
				android:layout_width="wrap_content">

				 

				<TextView
					android:paddingLeft="16dp"
					android:paddingRight="16dp"
					android:layout_weight="1.0"
					android:id="@+id/textview4"
					android:textColor="#FFFFFF"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="moonset"
					android:layout_below="@id/lin"
					android:layout_marginBottom="100dp"/>
				<TextView
					android:textSize="5sp"
					android:layout_height="wrap_content"
					android:layout_width="5dp"
					android:text="\t"/>
				<TextView
					android:paddingLeft="16dp"
					android:paddingRight="16dp"
					android:layout_weight="1.0"
					android:id="@+id/textview5"
					android:textColor="#9E9E9E"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="sunset"
					android:layout_below="@id/lin"
					android:layout_marginBottom="100dp"/>
			</TableRow>
		</TableLayout>

	</LinearLayout>

</LinearLayout>

